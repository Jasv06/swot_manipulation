git clone https://gitlab.com/sdurobotics/ur_rtde.git
ed ur rtde git checkout v1.5.6
git submodule update - -init --recursive mkdir build ca build
make - DCMAKE INSTALL PREFIX-/usr ..
make -j sudo make install

